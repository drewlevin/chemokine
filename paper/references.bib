title = {{Influenza-induced innate immunity: regulators of viral replication, respiratory tract pathology \& adaptive immunity}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3168512\&tool=pmcentrez\&rendertype=abstract},
volume = {6},
year = {2011}
}
@article{Osterlund2010,
abstract = {In less than 3 months after the first cases of swine origin 2009 influenza A (H1N1) virus infections were reported from Mexico, WHO declared a pandemic. The pandemic virus is antigenically distinct from seasonal influenza viruses, and the majority of human population lacks immunity against this virus. We have studied the activation of innate immune responses in pandemic virus-infected human monocyte-derived dendritic cells (DC) and macrophages. Pandemic A/Finland/553/2009 virus, representing a typical North American/European lineage virus, replicated very well in these cells. The pandemic virus, as well as the seasonal A/Brisbane/59/07 (H1N1) and A/New Caledonia/20/99 (H1N1) viruses, induced type I (alpha/beta interferon IFN-alpha/beta) and type III (IFN-lambda1 to -lambda3) IFN, CXCL10, and tumor necrosis factor alpha (TNF-alpha) gene expression weakly in DCs. Mouse-adapted A/WSN/33 (H1N1) and human A/Udorn/72 (H3N2) viruses, instead, induced efficiently the expression of antiviral and proinflammatory genes. Both IFN-alpha and IFN-beta inhibited the replication of the pandemic (H1N1) virus. The potential of IFN-lambda3 to inhibit viral replication was lower than that of type I IFNs. However, the pandemic virus was more sensitive to the antiviral IFN-lambda3 than the seasonal A/Brisbane/59/07 (H1N1) virus. The present study demonstrates that the novel pandemic (H1N1) influenza A virus can readily replicate in human primary DCs and macrophages and efficiently avoid the activation of innate antiviral responses. It is, however, highly sensitive to the antiviral actions of IFNs, which may provide us an additional means to treat severe cases of infection especially if significant drug resistance emerges.},
author = {Osterlund, Pamela and Pirhonen, Jaana and Ikonen, Niina and R\"{o}nkk\"{o}, Esa and Strengell, Mari and M\"{a}kel\"{a}, Sanna M and Broman, Mia and Hamming, Ole J and Hartmann, Rune and Ziegler, Thedi and Julkunen, Ilkka},
institution = {David H. Smith Center for Vaccine Biology and Immunology, University of Rochester Medical Center, 601 Elmwood Avenue, Box 609, Rochester, NY 14642, USA. Hailong\_Guo@urmc.rochester.edu},
journal = {Journal of Virology},
keywords = {antiviral agents,antiviral agents pharmacology,cells,cultured,cytokines,cytokines biosynthesis,dendritic cells,dendritic cells metabolism,enzyme inhibitors,enzyme inhibitors pharmacology,h1n1 subtype,h1n1 subtype drug effects,h1n1 subtype physiology,human,human epidemiology,human virology,humans,influenza,influenza a virus,interferon alpha,interferon alpha pharmacology,interferon beta,interferon beta pharmacology,macrophages,macrophages metabolism,neuraminidase,neuraminidase antagonists \& inhibitors,reverse transcriptase polymerase chain reaction,virus replication},
number = {3},
pages = {1414--1422},
publisher = {American Society for Microbiology (ASM)},
title = {{Pandemic H1N1 2009 influenza A virus induces weak cytokine responses in human macrophages and dendritic cells and is highly sensitive to the antiviral actions of interferons.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2812319\&tool=pmcentrez\&rendertype=abstract},
volume = {84},
year = {2010}
}
@article{Pawelek2012,
abstract = {Influenza virus infection remains a public health problem worldwide. The mechanisms underlying viral control during an uncomplicated influenza virus infection are not fully understood. Here, we developed a mathematical model including both innate and adaptive immune responses to study the within-host dynamics of equine influenza virus infection in horses. By comparing modeling predictions with both interferon and viral kinetic data, we examined the relative roles of target cell availability, and innate and adaptive immune responses in controlling the virus. Our results show that the rapid and substantial viral decline (about 2 to 4 logs within 1 day) after the peak can be explained by the killing of infected cells mediated by interferon activated cells, such as natural killer cells, during the innate immune response. After the viral load declines to a lower level, the loss of interferon-induced antiviral effect and an increased availability of target cells due to loss of the antiviral state can explain the observed short phase of viral plateau in which the viral level remains unchanged or even experiences a minor second peak in some animals. An adaptive immune response is needed in our model to explain the eventual viral clearance. This study provides a quantitative understanding of the biological factors that can explain the viral and interferon kinetics during a typical influenza virus infection.},
author = {Pawelek, Kasia A. and Huynh, Giao T. and Quinlivan, Michelle and Cullinane, Ann and Rong, Libin and Perelson, Alan S.},
file = {:nfs/adaptive/drew/Documents/Mendeley/Pawelek et al/PLoS Computational Biology/Pawelek et al. - 2012 - Modeling within-host dynamics of influenza virus infection including immune responses.pdf:pdf},
journal = {PLoS Computational Biology},
number = {6},
pmid = {22761567},
title = {{Modeling within-host dynamics of influenza virus infection including immune responses}},
volume = {8},
year = {2012}
}
