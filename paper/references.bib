journal = {PLoS Biology},
keywords = {b lymphocytes,b lymphocytes immunology,ccr7,cell communication,cell communication immunology,chemokine,chemokine immunology,chemotaxis,helper inducer,helper inducer immunology,humans,leukocyte,leukocyte immunology,ovalbumin,ovalbumin immunology,receptors,t lymphocytes},
