doi = {10.1371/journal.pone.0016245},
file = {:nfs/adaptive/drew/Documents/Mendeley/Purwar et al/PloS one/Purwar et al. - 2011 - Resident memory T cells (T(RM)) are abundant in human lung diversity, function, and antigen specificity.pdf:pdf},
issn = {1932-6203},
journal = {PloS one},
keywords = {Antigenic Variation,Antigens,Antigens: genetics,Antigens: immunology,Cytokines,Cytokines: immunology,Epitopes,Epitopes: immunology,Humans,Immunologic Memory,Immunologic Memory: immunology,Lung,Lung: cytology,Lung: immunology,Lymphocyte Count,Receptors, Antigen, T-Cell,Receptors, Antigen, T-Cell: immunology,T-Lymphocytes,T-Lymphocytes: cytology,T-Lymphocytes: immunology},
